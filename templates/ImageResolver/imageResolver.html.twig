{% extends 'base.html.twig' %}
{% block body %}
    {{ form_start(form) }}
        {{ form_row(form.categories) }}
        {{ form_row(form.get) }}
        {{ form_widget(form._token) }}
    {{ form_end(form, {'render_rest': false}) }}

    {% if image.path is not null %}
        <img src="{{ asset(image.path) }}" alt="">
    {% endif %}

{% endblock %}